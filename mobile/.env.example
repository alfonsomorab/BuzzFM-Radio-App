# App Configuration
APP_NAME=Radio App
APP_DESCRIPTION=A Flutter radio streaming app with MVVM architecture

# Package Names
ANDROID_PACKAGE_NAME=com.example.radio_app
IOS_BUNDLE_ID=com.example.radioapp

# API Configuration
API_BASE_URL=https://api.example.com
API_KEY=your_api_key_here

# Streaming Endpoints
# High Quality Stream
STREAM_URL_HIGH=https://streaming.hostpannel.lat:8012/stream
STREAM_BITRATE_HIGH=320
STREAM_QUALITY_HIGH=high

# Medium Quality Stream
STREAM_URL_MEDIUM=https://streaming.hostpannel.lat:2020/public/magicafm
STREAM_BITRATE_MEDIUM=128
STREAM_QUALITY_MEDIUM=medium

# Low Quality Stream (optional)
STREAM_URL_LOW=https://streaming.example.com/low
STREAM_BITRATE_LOW=64
STREAM_QUALITY_LOW=low

# App Settings
DEFAULT_STREAM_QUALITY=high
ENABLE_NOTIFICATIONS=true
ENABLE_BACKGROUND_PLAY=true

# Development Settings
DEBUG_MODE=true
LOG_LEVEL=info